    "prettier": "^3.2.5",
    "vite": "^5.0.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.3",
    "@types/react": "^18.2.41",
    "@types/react-dom": "^18.2.17"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-router-dom": "^6.20.1"
