{
  "compilerOptions": {
    "module": "ESNext",
    "target": "ES2018",
    "moduleResolution": "Bundler",
    "composite": true,
    "strictNullChecks": true,
    "strict": true,
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react",
    "allowImportingTsExtensions": true,
    "noEmit": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "album.type.d.ts"
  ]
}