{
  "compilerOptions": {
    "module": "ESNext",
    "target": "ES2018",
    "moduleResolution": "Bundler",
    "composite": true,
    "strictNullChecks": true,
    "strict": true,
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react",
    "allowImportingTsExtensions": false,
    "baseUrl": "."
  },
  "include": [
    "./**/*.ts",
    "./**/*"
  ]
}