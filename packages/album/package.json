{
  "name": "@albumjs/album",
  "version": "0.1.0",
  "description": "一个组合 vite+nestjs 的集成式框架",
  "keywords": [
    "@albumjs/album",
    "album"
  ],
  "files": [
    "dist",
    "types",
    "template"
  ],
  "homepage": "https://github.com/usagisah/album#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/usagisah/album.git"
  },
  "license": "MIT",
  "author": "顾弦笙/usagisah",
  "type": "module",
  "exports": {
    "./server": {
      "types": "./dist/server.d.ts",
      "default": "./dist/server.js"
    },
    "./service": {
      "types": "./dist/service.d.ts",
      "default": "./dist/service.js"
    },
    "./start": {
      "types": "./dist/start.d.ts",
      "default": "./dist/start.js"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "default": "./dist/config.js"
    },
    "./tools": {
      "types": "./dist/tools.d.ts",
      "default": "./dist/tools.js"
    },
    "./types/*": {
      "types": "./types/*.d.ts"
    }
  },
  "scripts": {
    "watch": "tsc -w"
  },
  "dependencies": {
    "@albumjs/tools": "workspace:^",
    "@nestjs/common": "^10.3.1",
    "@nestjs/core": "^10.3.1",
    "@nestjs/platform-express": "^10.3.1",
    "@ungap/structured-clone": "^1.2.0",
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "portfinder": "^1.0.32",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.1",
    "sirv": "^2.0.4",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "terser": "^5.27.0",
    "lightningcss": "^1.23.0",
    "@rspack/core": "^0.4.5",
    "vite": "^5.0.12",
    "vite-plugin-compression": "^0.5.1"
  },
  "devDependencies": {
    "@types/ungap__structured-clone": "^0.3.3",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/serve-static": "^1.15.5"
  }
}
