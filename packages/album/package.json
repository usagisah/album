{
  "name": "@albumjs/core",
  "version": "0.2.3",
  "description": "一个组合 vite+nestjs 的集成式框架",
  "keywords": [
    "@albumjs/core"
  ],
  "files": [
    "dist",
    "types"
  ],
  "homepage": "https://github.com/usagisah/album#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/usagisah/album.git"
  },
  "license": "MIT",
  "author": "顾弦笙/usagisah",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "default": "./dist/server.js"
    },
    "./start": {
      "types": "./dist/start.d.ts",
      "default": "./dist/start.js"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "default": "./dist/config.js"
    },
    "./utils/*": {
      "types": "./dist/utils/*.d.ts",
      "default": "./dist/utils/*.js"
    },
    "./types/*": {
      "types": "./types/*.d.ts"
    }
  },
  "bin": "./dist/cli/command.js",
  "scripts": {
    "watch": "tsc -p node/tsconfig.json -w & tsc -p client/tsconfig.json -w"
  },
  "dependencies": {
    "@albumjs/tools": "workspace:^",
    "@nestjs/common": "^10.3.1",
    "@nestjs/core": "^10.3.1",
    "@nestjs/platform-express": "^10.3.1",
    "@types/ungap__structured-clone": "^0.3.3",
    "@ungap/structured-clone": "^1.2.0",
    "colorette": "^2.0.20",
    "compression": "^1.7.4",
    "dayjs": "^1.11.10",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "mlly": "^1.5.0",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.1",
    "sirv": "^2.0.4",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@rspack/core": "^0.4.5",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/serve-static": "^1.15.5",
    "cac": "^6.7.14",
    "chokidar": "^3.5.3",
    "esbuild": "^0.19.12",
    "execa": "^8.0.1",
    "lightningcss": "^1.23.0",
    "pretty-format": "^29.7.0",
    "terser": "^5.27.0",
    "vite": "^5.0.12",
    "vite-plugin-compression": "^0.5.1"
  }
}
