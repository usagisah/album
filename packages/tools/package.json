{
  "name": "@albumjs/tools",
  "version": "0.2.0",
  "description": "工具箱",
  "keywords": [
    "@albumjs",
    "albumjs",
    "album",
    "@albumjs/tools"
  ],
  "homepage": "https://github.com/usagisah/album#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/usagisah/album.git"
  },
  "scripts": {
    "watch": "tsc -w"
  },
  "license": "MIT",
  "author": "顾弦笙/usagisah",
  "type": "module",
  "exports": {
    "./browser": {
      "types": "./dist/browser.d.ts",
      "import": "./dist/browser.js"
    },
    "./node": {
      "types": "./dist/node.d.ts",
      "import": "./dist/node.js"
    },
    "./bundle/*": {
      "types": "./dist/bundle/*.d.ts",
      "import": "./dist/bundle/*.js"
    },
    "./check/*": {
      "types": "./dist/check/*.d.ts",
      "import": "./dist/check/*.js"
    },
    "./fs/*": {
      "types": "./dist/fs/*.d.ts",
      "import": "./dist/fs/*.js"
    },
    "./modules/*": {
      "types": "./dist/modules/*.d.ts",
      "import": "./dist/modules/*.js"
    },
    "./path/*": {
      "types": "./dist/path/*.d.ts",
      "import": "./dist/path/*.js"
    },
    "./promises/*": {
      "types": "./dist/promises/*.d.ts",
      "import": "./dist/promises/*.js"
    },
    "./request/*": {
      "types": "./dist/request/*.d.ts",
      "import": "./dist/request/*.js"
    },
    "./types/*": {
      "types": "./dist/types/*.d.ts",
      "import": "./dist/types/*.js"
    },
    "./lib/*": {
      "types": "./dist/lib/*.d.ts",
      "import": "./dist/lib/*.js"
    }
  },
  "dependencies": {
    "@babel/core": "^7.23.5",
    "@babel/plugin-syntax-jsx": "^7.23.3",
    "@babel/plugin-syntax-typescript": "^7.23.3",
    "boxen": "^7.1.1",
    "cac": "^6.7.14",
    "chokidar": "^3.5.3",
    "colorette": "^2.0.20",
    "dayjs": "^1.11.10",
    "esbuild": "^0.19.8",
    "execa": "^8.0.1",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "inquirer": "^9.2.15",
    "magic-string": "^0.30.5",
    "minimist": "^1.2.8",
    "mlly": "^1.5.0",
    "ora": "^8.0.1",
    "pretty-bytes": "^6.1.1",
    "pretty-format": "^29.7.0",
    "resolve.exports": "^2.0.2",
    "serialize-javascript": "^6.0.2",
    "strip-ansi": "^7.1.0",
    "typescript": "^5.3.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.5",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/minimist": "^1.2.5",
    "@types/node": "^20.10.3",
    "@types/serialize-javascript": "^5.0.4"
  }
}
