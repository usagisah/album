{
  "name": "@w-hite/album",
  "version": "0.2.3",
  "description": "一个组合 vite+nestjs 的集成式框架",
  "keywords": [
    "@w-hite",
    "w-hite",
    "album",
    "@w-hite/album"
  ],
  "homepage": "https://github.com/usagisah/album#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/usagisah/album.git"
  },
  "license": "MIT",
  "author": "顾弦笙/usagisah",
  "type": "module",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js"
    },
    "./cli": {
      "types": "./cli.d.ts",
      "default": "./cli.js"
    },
    "./ssr": {
      "types": "./ssr.d.ts",
      "default": "./ssr.js"
    },
    "./server": {
      "types": "./server.d.ts",
      "default": "./server.js"
    },
    "./utils/*": {
      "types": "./utils/*.d.ts",
      "default": "./utils/*.js"
    },
    "./types/*": {
      "types": "./types/*.d.ts"
    }
  },
  "bin": "./cli/command.js",
  "scripts": {
    "build": "node ./scripts/build.mjs",
    "dev": "node ./scripts/dev.mjs",
    "format": "prettier --write \"node/**/*.ts\" \"client/**/*.ts\"",
    "pub": "node ./scripts/publish.mjs"
  },
  "dependencies": {
    "@nestjs/common": "^10.2.10",
    "@nestjs/core": "^10.2.10",
    "@nestjs/platform-express": "^10.2.10",
    "@types/ungap__structured-clone": "^0.3.3",
    "@ungap/structured-clone": "^1.2.0",
    "colorette": "^2.0.20",
    "compression": "^1.7.4",
    "dayjs": "^1.11.10",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "magic-string": "^0.30.5",
    "minimist": "^1.2.8",
    "mlly": "^1.4.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sirv": "^2.0.3",
    "typescript": "^5.3.2",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/plugin-syntax-jsx": "^7.23.3",
    "@babel/plugin-syntax-typescript": "^7.23.3",
    "@types/babel__core": "^7.20.5",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/minimist": "^1.2.5",
    "@types/node": "^20.10.3",
    "@types/serve-static": "^1.15.5",
    "cac": "^6.7.14",
    "chokidar": "^3.5.3",
    "esbuild": "^0.19.8",
    "execa": "^8.0.1",
    "lightningcss": "^1.22.1",
    "prettier": "^3.1.0",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-packagejson": "^2.4.7",
    "pretty-format": "^29.7.0",
    "resolve.exports": "^2.0.2",
    "terser": "^5.24.0",
    "vite": "^5.0.4",
    "vite-plugin-compression": "^0.5.1"
  }
}
