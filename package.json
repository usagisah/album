{
  "name": "@w-hite/album",
  "version": "0.2.3",
  "description": "一个组合 vite+nestjs 的集成式框架",
  "keywords": [
    "@w-hite",
    "w-hite",
    "album",
    "@w-hite/album"
  ],
  "homepage": "https://github.com/usagisah/album#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/usagisah/album.git"
  },
  "license": "MIT",
  "author": "顾弦笙/usagisah",
  "type": "module",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js"
    },
    "./cli": {
      "types": "./cli.d.ts",
      "default": "./cli.js"
    },
    "./ssr": {
      "types": "./ssr.d.ts",
      "default": "./ssr.js"
    },
    "./server": {
      "types": "./server.d.ts",
      "default": "./server.js"
    },
    "./utils/*": {
      "types": "./utils/*.d.ts",
      "default": "./utils/*.js"
    },
    "./types/*": {
      "types": "./types/*.d.ts"
    }
  },
  "bin": "./cli/command.js",
  "scripts": {
    "build": "node ./scripts/build.mjs",
    "dev": "node ./scripts/dev.mjs",
    "format": "prettier --write \"node/**/*.ts\" \"client/**/*.ts\"",
    "pub": "node ./scripts/publish.mjs"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "chokidar": "^3.5.3",
    "compression": "^1.7.4",
    "esbuild": "^0.19.3",
    "execa": "^8.0.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "less": "^4.2.0",
    "lightningcss": "^1.22.0",
    "minimist": "^1.2.8",
    "progress-webpack-plugin": "^1.0.16",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sass": "^1.69.3",
    "serve-static": "^1.15.0",
    "stylus": "^0.60.0",
    "swc-loader": "^0.2.3",
    "terser": "^5.22.0",
    "terser-webpack-plugin": "^5.3.9",
    "thread-loader": "^4.0.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-tsconfig-paths": "^4.2.1",
    "webpack": "^5.88.2",
    "webpack-node-externals": "^3.0.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.3",
    "@types/express": "^4.17.17",
    "@types/minimist": "^1.2.3",
    "@types/node": "^20.3.1",
    "@types/serve-static": "^1.15.3",
    "prettier": "^3.0.3",
    "prettier-config-usagisah": "^3.0.0",
    "prettier-plugin-organize-imports": "^3.2.3",
    "prettier-plugin-packagejson": "^2.4.6"
  }
}
