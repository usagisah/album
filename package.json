{
  "name": "@w-hite/album",
  "description": "一个组合 vite+nestjs 的集成式框架",
  "private": true,
  "license": "MIT",
  "author": "顾弦笙/usagisah",
  "type": "module",
  "scripts": {
    "init": "node ./scripts/init.mjs",
    "preBuild": "node ./scripts/preBuild.mjs",
    "watch:react": "npm run preBuild && concurrently 'cd packages/tools && npm run watch' 'cd packages/album && npm run watch' 'cd packages/plugin-react && npm run watch'",
    "format": "prettier --write \"packages/**/*.ts\" \"e2e/**/*.{ts,tsx}\"",
    "test:e2e": "vitest run -c e2e/vitest.config.ts",
    "test:e2e:react-start": "vitest run -c e2e/vitest.config.ts react/__test__/start",
    "pub": "node ./scripts/publish.mjs"
  },
  "devDependencies": {
    "@types/node": "^20.10.3",
    "colorette": "^2.0.20",
    "esbuild": "^0.19.8",
    "execa": "^8.0.1",
    "prettier": "^3.1.0",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-packagejson": "^2.4.7",
    "pretty-format": "^29.7.0",
    "puppeteer": "^21.11.0",
    "vitest": "^1.2.2",
    "concurrently": "^8.2.2",
    "typescript": "^5.3.2"
  }
}
